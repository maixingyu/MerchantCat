<cu-custom bgColor="bg-gradual-orange" isBack="{{true}}">
  <view slot="backText">订单</view>
</cu-custom>
<view class="cu-list menu sm-border">
  <view class="cu-item" wx:for="{{merchantOrderList}}" wx:key="index">
    <view class="content padding-tb-sm ">
      <view class="text-black text-sm ">
        <text class="cuIcon-formfill margin-right-xs text-orange"></text>订单号：{{item.orderNumber}}</view>
      <view class="text-black text-sm ">
        <text class="cuIcon-moneybagfill margin-right-xs text-orange"></text>金额：{{item.money}}元</view>
      <view class="text-black text-sm ">
        <text class="cuIcon-calendar margin-right-xs text-orange"></text>时间：{{item.time}}</view>
    </view>
  </view>
</view>